				ledF2Top <= '0';
				ledGoingDwTop <= '0';
				ledGoingUpTop <= '0';
				ledF0Door <= '0';
				ledF1DoorDw <= '0';
				ledF1DoorUp <= '0';
				ledF2Door <= '0';
				ledF0Panel <= '1';
				ledF1Panel <= '0';
				ledF2Panel <= '0';
				ledEmergenciaPanel <= '0';
		when 32 => --parar en 2 para bajar a 1
				motorUp <= '0';
				motorDw <= '0';
				motorSlow <= '0';
				doorF0Open <= '0';
				doorF0Close <= '1';
				doorF1Open <= '0';
				doorF1Close <= '1';
				doorF2Open <= '1';
				doorF2Close <= '0';
				ledF0Top <= '0';
				ledF1Top <= '0';
				ledF2Top <= '1';
				ledGoingDwTop <= '0';
				ledGoingUpTop <= '0';
				ledF0Door <= '0';
				if boton1DwS='0' then
					ledF1DoorDw <= '1';
				else
					ledF1DoorDw <= '0';
				end if;
				ledF1DoorUp <= '0';
				ledF2Door <= '0';
				ledF0Panel <= '0';
				if botonF1S='0' then
					ledF1Panel <= '1';
				else
					ledF1Panel <= '0';
				end if;
				ledF2Panel <= '0';
				ledEmergenciaPanel <= '0';
		when 33 => --espera en 2 para bajar a 1
				motorUp <= '0';
				motorDw <= '0';
				motorSlow <= '0';
				doorF0Open <= '0';
				doorF0Close <= '1';
				doorF1Open <= '0';
				doorF1Close <= '1';
				doorF2Open <= '1';
				doorF2Close <= '0';
				ledF0Top <= '0';
				ledF1Top <= '0';
				ledF2Top <= '1';
				ledGoingDwTop <= '0';
				ledGoingUpTop <= '0';
				ledF0Door <= '0';
				if boton1DwS='0' then
					ledF1DoorDw <= '1';
				else
					ledF1DoorDw <= '0';
				end if;
				ledF1DoorUp <= '0';
				ledF2Door <= '0';
				ledF0Panel <= '0';
				if botonF1S='0' then
					ledF1Panel <= '1';
				else
					ledF1Panel <= '0';
				end if;
				ledF2Panel <= '0';
				ledEmergenciaPanel <= '0';
		when 34 => --parar en 2 para bajar a 1
				motorUp <= '0';
				motorDw <= '0';
				motorSlow <= '0';
				doorF0Open <= '0';
				doorF0Close <= '1';
				doorF1Open <= '0';
				doorF1Close <= '1';
				doorF2Open <= '1';
				doorF2Close <= '0';
				ledF0Top <= '0';
				ledF1Top <= '0';
				ledF2Top <= '1';
				ledGoingDwTop <= '0';
				ledGoingUpTop <= '0';
				ledF0Door <= '0';
				ledF1DoorDw <= '0';
				ledF1DoorUp <= '0';
				ledF2Door <= '0';
				ledF0Panel <= '1';
				ledF1Panel <= '0';
				ledF2Panel <= '0';
				ledEmergenciaPanel <= '0';
		when 35 => --espera en 2 para bajar a 1
				motorUp <= '0';
				motorDw <= '0';
				motorSlow <= '0';
				doorF0Open <= '0';
				doorF0Close <= '1';
				doorF1Open <= '0';
				doorF1Close <= '1';
				doorF2Open <= '1';
				doorF2Close <= '0';
				ledF0Top <= '0';
				ledF1Top <= '0';
				ledF2Top <= '1';
				ledGoingDwTop <= '0';
				ledGoingUpTop <= '0';
				ledF0Door <= '0';
				ledF1DoorDw <= '0';
				ledF1DoorUp <= '0';
				ledF2Door <= '0';
				ledF0Panel <= '1';
				ledF1Panel <= '0';
				ledF2Panel <= '0';
				ledEmergenciaPanel <= '0';
		when others =>
--				motores <= '0';
--				sensores <= '0';
				motorUp<='0';
				motorDw<='0';
				motorSlow <= '0';
				doorF0Open <= '0';
				doorF0Close <= '1';
				doorF1Open <= '0';
				doorF1Close <= '1';
				doorF2Open <= '0';
				doorF2Close <= '1';
				ledF0Top <= '0';
				ledF1Top <= '0';
				ledF2Top <= '0';
				ledGoingDwTop <= '0';
				ledGoingUpTop <= '0';
				ledF0Door <= '0';
				ledF1DoorDw <= '0';
				ledF1DoorUp <= '0';
				ledF2Door <= '0';
				ledF0Panel <= '0';
				ledF1Panel <= '0';
				ledF2Panel <= '0';
				ledEmergenciaPanel <= '0';
	end case;
end process;

memoria_botones_panel:
PROCESS(clk, cabinInF0, doorF0Opened, cabinInF1, doorF1Opened, cabinInF2, doorF2Opened)-- Captura la pulsaci贸n del bot贸n
BEGIN
   IF clk='1' AND clk'event THEN
		if divisor=0 then
      IF (botonF0='0') THEN 
			botonF0S <= botonF0;
		elsif (botonF1='0') then
			botonF1S <= botonF1;
		elsif (botonF2='0') then
			botonF2S <= botonF2;
		elsif cabinInF2 = '1' and doorF2Opened = '1' THEN
			botonF2S<='1';
		elsif cabinInF1 = '1' and doorF1Opened = '1' then
			botonF1S<='1';
		elsif cabinInF0 = '1' and doorF0Opened = '1' then
			botonF0S<='1';
      END IF;
	  end if;
   END IF;
END PROCESS memoria_botones_panel;

memoria_botones_planta:
PROCESS(clk, cabinInF0, doorF0Opened, cabinInF1, doorF1Opened, cabinInF2, doorF2Opened)-- Captura la pulsaci贸n del bot贸n
BEGIN
   IF clk='1' AND clk'event THEN
		if divisor=0 then
      IF (boton0='0') THEN 
			boton0S <= boton0;
		elsif (boton1Dw='0') then
			boton1DwS <= boton1Dw;
		elsif (boton1Up='0') then
			boton1UpS <= boton1Up;
		elsif (boton2='0') then
			boton2S <= boton2;
		elsif cabinInF2 = '1' and doorF2Opened = '1' THEN
			boton2S<='1';
		elsif cabinInF1 = '1' and doorF1Opened = '1' then
			boton1UpS<='1';
			boton1DwS<='1';
		elsif cabinInF0 = '1' and doorF0Opened = '1' then
			boton0S<='1';
      END IF;
	  end if;
   END IF;
END PROCESS memoria_botones_planta;

memoria_cabina:
PROCESS(clk, cabinInF0, doorF0Opened, cabinInF1, doorF1Opened, cabinInF2, doorF2Opened)
BEGIN
   IF clk='1' AND clk'event THEN
		if divisor=0 then
      IF (cabinInF0='1' and doorF0Opened='1') THEN 
			cabinInF0S <= cabinInF0;
			cabinInF1S <= '0';
			cabinInF2S <= '0';
		elsif (cabinInF1='1' and doorF1Opened='1') then
			cabinInF1S <= cabinInF1;
			cabinInF0S <= '0';
			cabinInF2S <= '0';
		elsif (cabinInF2='1' and doorF2Opened='1') then
			cabinInF2S <= cabinInF2;
			cabinInF0S <= '0';
			cabinInF1S <= '0';
      END IF;
	  end if;
   END IF;
END PROCESS memoria_cabina;

cont_aux_0 <=contador_aux_0;
cont_aux_1 <=contador_aux_1;
cont_aux_2 <=contador_aux_2;
ledAlarmaPanel <= pulso;
end Behavioral;
